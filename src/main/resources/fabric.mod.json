{
  "schemaVersion": 1,
  "id": "squared",
  "version": "${version}",

  "name": "Squared Client",
  "description": "Squared is a all-in-one, free and open-source, with essential mods like FPS counters, shader support, keystrokes, and so much more.",
  "authors": [
    "AceiusIO"
  ],
  "contact": {
    "homepage": "https://squared.acei.us/",
    "sources": "https://github.com/AceiusIO/squared-fabric"
  },

  "license": "CC0-1.0",
  "icon": "assets/squared/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "us.acei.squared.SquaredFabric"
    ]
  },
  "mixins": [
    "squared.mixins.json"
  ],

  "depends": {
    "fabric-key-binding-api-v1": "*",
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "sodium": "*",
    "iris": "*"
  }
}
